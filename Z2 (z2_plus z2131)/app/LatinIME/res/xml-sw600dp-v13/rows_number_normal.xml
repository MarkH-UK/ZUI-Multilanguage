<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <Row>
        <Key latin:backgroundType="functional" latin:keySpec="-" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keySpec="+" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keySpec="." latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:keySpec="1" latin:keyStyle="numKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keySpec="2" latin:keyStyle="numKeyStyle" />
        <Key latin:keySpec="3" latin:keyStyle="numKeyStyle" />
        <Key latin:keyStyle="deleteKeyStyle" latin:keyWidth="fillRight" latin:keyXPos="-13.999998%p" />
    </Row>
    <Row>
        <Key latin:backgroundType="functional" latin:keySpec="＊|*" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keySpec="/" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <switch>
            <case latin:mode="time|datetime">
                <Key latin:moreKeys="!text/morekeys_am_pm" latin:backgroundType="functional" latin:keySpec="," latin:keyLabelFlags="hasPopupHint" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
            </case>
            <default>
                <Key latin:backgroundType="functional" latin:keySpec="," latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
            </default>
        </switch>
        <Key latin:keySpec="4" latin:keyStyle="numKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keySpec="5" latin:keyStyle="numKeyStyle" />
        <Key latin:keySpec="6" latin:keyStyle="numKeyStyle" />
        <Key latin:keyStyle="enterKeyStyle" latin:keyWidth="fillRight" latin:keyXPos="-13.999998%p" />
    </Row>
    <Row>
        <Key latin:backgroundType="functional" latin:keySpec="(" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <Key latin:backgroundType="functional" latin:keySpec=")" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
        <switch>
            <case latin:mode="time|datetime">
                <Key latin:backgroundType="functional" latin:keySpec=":" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
            </case>
            <default>
                <Key latin:backgroundType="functional" latin:keySpec="=" latin:keyStyle="numSymbolKeyStyle" latin:keyWidth="10.000002%p" />
            </default>
        </switch>
        <Key latin:keySpec="7" latin:keyStyle="numKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keySpec="8" latin:keyStyle="numKeyStyle" />
        <Key latin:keySpec="9" latin:keyStyle="numKeyStyle" />
        <Spacer />
    </Row>
    <Row>
        <Key latin:keyStyle="tabletNumSpaceKeyStyle" latin:keyWidth="29.999996%p" />
        <Key latin:keySpec="＊|*" latin:keyStyle="numSymbolKeyStyle" latin:keyXPos="31.000006%p" />
        <Key latin:keySpec="0" latin:keyStyle="numKeyStyle" />
        <Key latin:keySpec="\#" latin:keyStyle="numKeyStyle" />
        <Spacer latin:keyWidth="fillRight" />
    </Row>
</merge>