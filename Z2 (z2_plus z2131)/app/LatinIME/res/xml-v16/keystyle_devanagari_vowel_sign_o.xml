<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSet="hindi">
            <key-style latin:moreKeys="ों,ॉ,ॊ" latin:styleName="moreKeysDevanagariVowelSignO" />
        </case>
        <case latin:keyboardLayoutSet="hindi_compact">
            <key-style latin:moreKeys="ो" latin:styleName="moreKeysDevanagariVowelSignO" />
        </case>
        <case latin:keyboardLayoutSet="marathi">
            <key-style latin:moreKeys="ओ" latin:styleName="moreKeysDevanagariVowelSignO" />
        </case>
        <default>
            <key-style latin:styleName="moreKeysDevanagariVowelSignO" />
        </default>
    </switch>
    <key-style latin:keySpec="ो" latin:styleName="baseKeyDevanagariVowelSignO" latin:parentStyle="moreKeysDevanagariVowelSignO" />
</merge>