<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSet="hindi">
            <key-style latin:moreKeys="◌ॽ|ॽ,◌॰|॰,◌ऽ|ऽ" latin:styleName="moreKeysDevanagariSignNukta" />
        </case>
        <case latin:keyboardLayoutSet="nepali_romanized">
            <key-style latin:moreKeys="◌़|़" latin:styleName="moreKeysDevanagariSignNukta" />
        </case>
        <default>
            <key-style latin:styleName="moreKeysDevanagariSignNukta" />
        </default>
    </switch>
    <key-style latin:keySpec="◌़|़" latin:keyLabelFlags="followKeyLetterRatio" latin:styleName="baseKeyDevanagariSignNukta" latin:parentStyle="moreKeysDevanagariSignNukta" />
</merge>