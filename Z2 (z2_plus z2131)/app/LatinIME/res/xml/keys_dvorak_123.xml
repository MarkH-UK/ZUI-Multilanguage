<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:additionalMoreKeys="1" latin:keySpec="&quot;" latin:keyHintLabel="1" />
        </case>
        <case latin:mode="url">
            <Key latin:additionalMoreKeys="1" latin:keySpec="/" latin:keyHintLabel="1" />
        </case>
        <case latin:mode="email">
            <Key latin:additionalMoreKeys="1" latin:keySpec="\@" latin:keyHintLabel="1" />
        </case>
        <default>
            <Key latin:moreKeys="!,&quot;" latin:additionalMoreKeys="1" latin:keySpec="'" latin:keyHintLabel="1" />
        </default>
    </switch>
    <switch>
        <case latin:keyboardLayoutSetElement="alphabetManualShifted|alphabetShiftLocked|alphabetShiftLockShifted">
            <Key latin:additionalMoreKeys="2" latin:keySpec="&lt;" latin:keyHintLabel="2" />
            <Key latin:additionalMoreKeys="3" latin:keySpec=">" latin:keyHintLabel="3" />
        </case>
        <default>
            <Key latin:moreKeys="\?,&lt;" latin:additionalMoreKeys="2" latin:keySpec="," latin:keyHintLabel="2" />
            <Key latin:moreKeys=">" latin:additionalMoreKeys="3" latin:keySpec="." latin:keyHintLabel="3" />
        </default>
    </switch>
</merge>