<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/com.android.inputmethod.latin">
    <switch>
        <case latin:keyboardLayoutSet="hindi">
            <key-style latin:moreKeys="◌ौं|ौं,%" latin:styleName="moreKeysDevanagariVowelSignAu" />
        </case>
        <case latin:keyboardLayoutSet="hindi_compact">
            <key-style latin:moreKeys="◌ौ|ौ,%" latin:styleName="moreKeysDevanagariVowelSignAu" />
        </case>
        <case latin:keyboardLayoutSet="marathi">
            <key-style latin:moreKeys="औ,%" latin:styleName="moreKeysDevanagariVowelSignAu" />
        </case>
        <default>
            <key-style latin:styleName="moreKeysDevanagariVowelSignAu" />
        </default>
    </switch>
    <key-style latin:keySpec="◌ौ|ौ" latin:keyLabelFlags="followKeyLetterRatio" latin:styleName="baseKeyDevanagariVowelSignAu" latin:parentStyle="moreKeysDevanagariVowelSignAu" />
</merge>